{
    "manifest_version": 3,
    "name": "ViUWS Browser",
    "version": "1.0",
    "action": {
        "default_icon": {
            "16": "icons/viuws_16x16.png",
            "24": "icons/viuws_24x24.png",
            "32": "icons/viuws_32x32.png",
            "48": "icons/viuws_48x48.png",
            "64": "icons/viuws_64x64.png",
            "96": "icons/viuws_96x96.png",
            "128": "icons/viuws_128x128.png"
        },
        "default_title": "ViUWS Browser",
        "default_popup": "popup/popup.html"
    },
    "description": "Visual Uppsala Workflow System (ViUWS)",
    "icons": {
        "16": "icons/viuws_16x16.png",
        "24": "icons/viuws_24x24.png",
        "32": "icons/viuws_32x32.png",
        "48": "icons/viuws_48x48.png",
        "64": "icons/viuws_64x64.png",
        "96": "icons/viuws_96x96.png",
        "128": "icons/viuws_128x128.png"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "permissions": [
        "nativeMessaging"
    ]
}